{
	"compilerOptions": {
		"target": "es5",
		"module": "esnext",
		"strict": true,
		"jsx": "preserve",
		"importHelpers": true,
		"moduleResolution": "node",
		"allowJs": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"sourceMap": true,
		"noImplicitThis": false,
		"baseUrl": "./js",
		"resolveJsonModule": true,
		"paths": {
			"@/*": ["admin/*"],
			"#/*": ["front/*"]
		},
		"lib": [
			"ES2015",
			"ES2016",
			"ES2017",
			"ES2018",
			"ES2019",
			"ES2020",
			"ESNext",
			"dom",
			"dom.iterable",
			"scripthost"
		]
	},
	"include": ["./**/*.ts", "./**/*.tsx", "./**/*.vue", "./**/*.json"],
	"exclude": ["node_modules"]
}
